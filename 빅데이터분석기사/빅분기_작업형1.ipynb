        "import pandas as pd\n",
        "from sklearn.preprocessing import MinMaxScaler\n",
        "\n",
        "data = pd.read_csv('data/mtcars.csv')\n",
        "\n",
        "s=pd.DataFrame(data['qsec'])\n",
        "\n",
        "scaler = MinMaxScaler()\n",
        "scaler.fit(s)\n",
        "\n",
        "s=scaler.transform(s)\n",
        "count=0\n",
        "for c in s:\n",
        "\tif c>0.5:\n",
        "\t\tcount+=1\n",
        "print(count)"
      ],
